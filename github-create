github-create (){
  if [ $1 ]
	then
    repo_name=$1
  else
    echo "Enter repo name:"
    read repo_name
  fi

  curl -i -u 'a-a-ron:85e3b8f6606d413288f4b4550a2d15a82799114c' -d '{"name":"'$repo_name'"}' https://api.github.com/user/repos
  git remote add origin git@github.com:efatsi/$repo_name.git
  git push -u origin master
}
